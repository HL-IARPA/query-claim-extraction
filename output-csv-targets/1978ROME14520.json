{
  "doc_id": "1978ROME14520",
  "doc_subject": "LIBYAN INVOLVEMENT IN TERRORIST INCIDENT",
  "doc_date": "1978",
  "claims": [
    {
      "claim_id": "c1",
      "claim_text": "The USG followed up an earlier demarche (Ref B) with MFA Director for Middle Eastern Affairs Santarelli on August 7, 1978.",
      "claim_type": "event",
      "entities": [
        "USG",
        "MFA Director for Middle Eastern Affairs Santarelli"
      ],
      "time_bounds": {
        "start": "1978-08-07",
        "end": null
      },
      "importance": 3
    },
    {
      "claim_id": "c2",
      "claim_text": "Santarelli said that the French informed the Italians about the origin of weapons used in a terrorist incident at Orly.",
      "claim_type": "attribution",
      "entities": [
        "Santarelli",
        "French",
        "Italians",
        "Orly"
      ],
      "time_bounds": {
        "start": null,
        "end": "1978-08-07"
      },
      "importance": 4
    },
    {
      "claim_id": "c3",
      "claim_text": "Santarelli said that the MFA instructed the Italian Ambassador in Tripoli to raise the issue with the Libyans.",
      "claim_type": "attribution",
      "entities": [
        "Santarelli",
        "MFA",
        "Italian Ambassador",
        "Tripoli",
        "Libyans"
      ],
      "time_bounds": {
        "start": null,
        "end": "1978-08-07"
      },
      "importance": 4
    },
    {
      "claim_id": "c4",
      "claim_text": "The GOL has not yet responded to the demarche.",
      "claim_type": "event",
      "entities": [
        "GOL"
      ],
      "time_bounds": {
        "start": "1978-08-07",
        "end": null
      },
      "importance": 3
    },
    {
      "claim_id": "c5",
      "claim_text": "The Italians are satisfied that the GOL knows that the GOI knows of Libyan supply of Italian arms to terrorists.",
      "claim_type": "assessment",
      "entities": [
        "Italians",
        "GOL",
        "GOI",
        "Libyan",
        "terrorists"
      ],
      "time_bounds": {
        "start": "1978-08-07",
        "end": null
      },
      "importance": 5
    },
    {
      "claim_id": "c6",
      "claim_text": "Santarelli cautioned that the foregoing information is considered very sensitive in the GOI.",
      "claim_type": "attribution",
      "entities": [
        "Santarelli",
        "GOI"
      ],
      "time_bounds": {
        "start": "1978-08-07",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c7",
      "claim_text": "Santarelli cautioned that the foregoing information is for USG information only.",
      "claim_type": "attribution",
      "entities": [
        "Santarelli",
        "USG"
      ],
      "time_bounds": {
        "start": "1978-08-07",
        "end": null
      },
      "importance": 3
    },
    {
      "claim_id": "c8",
      "claim_text": "Santarelli cautioned that the foregoing information should not be discussed with Italian diplomats elsewhere or with other governments.",
      "claim_type": "attribution",
      "entities": [
        "Santarelli",
        "Italian diplomats",
        "other governments"
      ],
      "time_bounds": {
        "start": "1978-08-07",
        "end": null
      },
      "importance": 3
    },
    {
      "claim_id": "c9",
      "claim_text": "Santarelli said the MFA is not clear about exactly what is intended regarding a joint demarche.",
      "claim_type": "attribution",
      "entities": [
        "Santarelli",
        "MFA"
      ],
      "time_bounds": {
        "start": "1978-08-07",
        "end": null
      },
      "importance": 3
    },
    {
      "claim_id": "c10",
      "claim_text": "The Italians are not necessarily opposed to the idea of a further demarche.",
      "claim_type": "assessment",
      "entities": [
        "Italians"
      ],
      "time_bounds": {
        "start": "1978-08-07",
        "end": null
      },
      "importance": 3
    },
    {
      "claim_id": "c11",
      "claim_text": "The Italians feel that they have already registered their reaction in a bilateral approach.",
      "claim_type": "assessment",
      "entities": [
        "Italians"
      ],
      "time_bounds": {
        "start": "1978-08-07",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c12",
      "claim_text": "For political and economic reasons, the USG does not believe the Italians would be willing to join in a \"tough demarche\" to the Libyans on this issue.",
      "claim_type": "assessment",
      "entities": [
        "USG",
        "Italians",
        "Libyans"
      ],
      "time_bounds": {
        "start": "1978",
        "end": null
      },
      "importance": 5
    },
    {
      "claim_id": "c13",
      "claim_text": "From the Italian point of view, they have already taken a courageous step in confronting the GOL with their knowledge of weapons supply.",
      "claim_type": "assessment",
      "entities": [
        "Italians",
        "GOL"
      ],
      "time_bounds": {
        "start": "1978",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c14",
      "claim_text": "The Italians are not likely to want to pursue the issue with the GOL any further.",
      "claim_type": "assessment",
      "entities": [
        "Italians",
        "GOL"
      ],
      "time_bounds": {
        "start": "1978",
        "end": null
      },
      "importance": 5
    }
  ],
