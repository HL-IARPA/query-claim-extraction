  "doc_subject": "MALTA - ANNUAL POLICY ASSESSMENT 1976",
  "doc_date": "1976",
  "claims": [
    {
      "claim_id": "c1",
      "claim_text": "1976 is an election year in Malta.",
      "claim_type": "event",
      "entities": [
        "Malta"
      ],
      "time_bounds": {
        "start": "1976",
        "end": "1976"
      },
      "importance": 3
    },
    {
      "claim_id": "c2",
      "claim_text": "Both Mintoff's Laborites and the opposition Nationalists believe that Malta's strategic Mediterranean location will still be sufficiently valuable to NATO to justify continuation after 1979 of some form of annual monetary subsidy.",
      "claim_type": "attribution",
      "entities": [
        "Mintoff's Laborites",
        "Nationalists",
        "Malta",
        "NATO"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 5
    },
    {
      "claim_id": "c3",
      "claim_text": "The USG has serious doubts that Malta's strategic Mediterranean location will still be sufficiently valuable to NATO to justify continuation after 1979 of some form of annual monetary subsidy.",
      "claim_type": "assessment",
      "entities": [
        "USG",
        "Malta",
        "NATO"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 5
    },
    {
      "claim_id": "c4",
      "claim_text": "It will be essential for the USG and NATO to reach conclusions concerning Malta's future as soon as possible after the 1976 elections to avoid another crisis management exercise resulting from Maltese blackmailing tactics.",
      "claim_type": "plan",
      "entities": [
        "USG",
        "NATO",
        "Malta"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c5",
      "claim_text": "Bilateral U.S. interests in Malta have been, and will remain, limited.",
      "claim_type": "assessment",
      "entities": [
        "U.S.",
        "Malta"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c6",
      "claim_text": "If NATO's Southern Tier remains intact, Malta in the 1980's might not matter that much to the other Alliance members either.",
      "claim_type": "assessment",
      "entities": [
        "NATO",
        "Malta",
        "Alliance members"
      ],
      "time_bounds": {
        "start": "1980",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c7",
      "claim_text": "The 325,000 Maltese appear to be fairly evenly divided between support for Mintoff's Socialist Labor Party and the conservative Nationalist Party.",
      "claim_type": "assessment",
      "entities": [
        "Maltese",
        "Mintoff's Socialist Labor Party",
        "Nationalist Party"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 3
    },
    {
      "claim_id": "c8",
      "claim_text": "The Maltese Parliament's mandate expires in August 1976, and a general election is required by November 1976.",
      "claim_type": "logistics",
      "entities": [
        "Maltese Parliament",
        "Malta"
      ],
      "time_bounds": {
        "start": "1976-08",
        "end": "1976-11"
      },
      "importance": 3
    },
    {
      "claim_id": "c9",
      "claim_text": "The Embassy leans to the view that Mintoff and the Labor Party probably will be reelected in a very close contest.",
      "claim_type": "attribution",
      "entities": [
        "Embassy",
        "Mintoff",
        "Labor Party"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c10",
      "claim_text": "If the Nationalists return to power, there will be an opportunity for closer ties between Malta and the U.S. and possibly for resumption of Sixth Fleet visits, but only at a stiff price.",
      "claim_type": "assessment",
      "entities": [
        "Nationalists",
        "Malta",
        "U.S.",
        "Sixth Fleet"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c11",
      "claim_text": "The major U.S. interest in Malta is not a bilateral one, but part of the multilateral concern of the NATO Alliance regarding Mediterranean security.",
      "claim_type": "assessment",
      "entities": [
        "U.S.",
        "Malta",
        "NATO Alliance"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c12",
      "claim_text": "The major U.S. policy objectives in Malta include denial of Soviet access to Malta, retention of Malta's military facilities for the West (including Sixth Fleet access), and continuation of Malta's ties to the West.",
      "claim_type": "plan",
      "entities": [
        "U.S.",
        "Malta",
        "Soviet Union",
        "West",
        "Sixth Fleet"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 5
    },
    {
      "claim_id": "c13",
      "claim_text": "Mintoff's foreign policy continues to be the avoidance of involvement with either of the superpowers; hence, neither Soviet nor U.S. fleets are welcome in Malta.",
      "claim_type": "assessment",
      "entities": [
        "Mintoff",
        "Soviet Union",
        "U.S.",
        "Malta"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c14",
      "claim_text": "The revised British/Maltese Military Facilities Agreement (MFA), signed in March 1972, runs until March 31, 1979.",
      "claim_type": "logistics",
      "entities": [
        "British",
        "Maltese",
        "Military Facilities Agreement (MFA)"
      ],
      "time_bounds": {
        "start": "1972-03",
        "end": "1979-03-31"
      },
      "importance": 4
    },
    {
      "claim_id": "c15",
      "claim_text": "The MFA specifically provides that the Government of Malta shall not permit the forces of any Warsaw Pact member to be stationed in Malta or to use Maltese military facilities.",
      "claim_type": "logistics",
      "entities": [
        "Government of Malta",
        "Warsaw Pact",
        "Malta"
      ],
      "time_bounds": {
        "start": "1972-03",
        "end": "1979-03-31"
      },
      "importance": 5
    },
    {
      "claim_id": "c16",
      "claim_text": "There continues to be a complete denial of access to Malta by any U.S. military or naval forces, including the Sixth Fleet, as long as Mintoff is Prime Minister.",
      "claim_type": "assessment",
      "entities": [
        "Malta",
        "U.S. military forces",
        "U.S. naval forces",
        "Sixth Fleet",
        "Mintoff"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c17",
      "claim_text": "Neither the Labor Party nor the Nationalist Party really has firm plans for what happens to Malta after 1977 or after the March 1979 expiration of the MFA.",
      "claim_type": "assessment",
      "entities": [
        "Labor Party",
        "Nationalist Party",
        "Malta",
        "MFA"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 5
    },
    {
      "claim_id": "c18",
      "claim_text": "Malta's economic viability is clearly dependent on either some form of continued monetary subsidy or a significant new development like the discovery of offshore oil.",
      "claim_type": "assessment",
      "entities": [
        "Malta"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c19",
      "claim_text": "The British have stated that their decision to leave Malta by 1979 is firm and irrevocable.",
      "claim_type": "attribution",
      "entities": [
        "British",
        "Malta"
      ],
      "time_bounds": {
        "start": "1976",
        "end": "1979"
      },
      "importance": 5
    },
    {
      "claim_id": "c20",
      "claim_text": "The Embassy recommends the USG and other NATO governments should avoid any attempts to buy Mintoff's favor before the election, as such initiatives would be wasted and seen as taking sides.",
      "claim_type": "plan",
      "entities": [
        "Embassy",
        "USG",
        "NATO governments",
        "Mintoff"
      ],
      "time_bounds": {
        "start": "1976",
        "end": null
      },
      "importance": 4
    }
