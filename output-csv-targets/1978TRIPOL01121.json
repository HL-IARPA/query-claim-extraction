      "entities": [
        "arms",
        "terrorists",
        "Italy",
        "Libya"
      ],
      "time_bounds": {
        "start": null,
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c3",
      "claim_text": "The Libyans could easily side-step the issue of arms purchase by observing that their long-term support for Palestinian resistance movements, including provision of arms, is no secret.",
      "claim_type": "assessment",
      "entities": [
        "Libyans",
        "Palestinian resistance movements",
        "arms"
      ],
      "time_bounds": {
        "start": "1978",
        "end": null
      },
      "importance": 4
    },
    {
      "claim_id": "c4",
      "claim_text": "The Libyans could easily side-step the issue of arms purchase by observing that how Palestinians use such arms is not Libyan responsibility.",
